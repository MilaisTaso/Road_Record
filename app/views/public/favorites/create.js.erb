$(".favorite_<%= @course.id %>").html(
"<%= j(render 'public/favorites/button', course: @course) %>");
$(".finish_<%= @course.id %>").html(
"<%= j(render 'public/finishes/button', course: @course) %>");

$("#favorite-tag").html(
"<%= j(render 'public/favorites/favorite_nav', user: @course.user) %>");
